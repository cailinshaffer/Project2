<!-- show results from search page -->
<%- include('../partials/header', {user}) %>
<h1>Look at all these fur babies! </h1>
<style>
  body{
    background: url(https://coolbackgrounds.io/images/backgrounds/index/ranger-4df6c1b6.png);
    background-size: cover;
    background-position: center;
  }
</style>

<% pets.forEach(pet => { %>
    <% if (pet.photos.length > 0) { %>
    <div>
        

        <a href="/pets/<%=pet.id%>"><h2><%= pet.name %></h2></a>

        
        <img alt="<%= pet.type %>" src="<%= pet.photos[0].medium %>">
        
      <% }%>  
        </div>
        <%}) %>
        


